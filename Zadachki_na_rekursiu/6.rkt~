#lang racket
(define (Remove2 L)
  (if (null? L)
      L
      (if (empty? (cdr L))
          L
          (cons (car L) (Remove2 (cdr (cdr L)))))))

(Remove2 '(1 2 3 4 5 6 7 8 9))
;Написать функцию (Remove2 L), удаляющую из списка каждый
;второй элемент верхнего уровня:
;(Remove2 '(A B C D E)) => (A C E).